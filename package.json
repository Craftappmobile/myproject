{
  "name": "expo-supabase-starter",
  "version": "1.0.0",
  "main": "./index.js",
  "scripts": {
    "start": "npx expo start",
    "start:clear": "npx expo start --clear",
    "start:port": "npx expo start --port 8083",
    "start:reset": "npx expo start --clear --port 8083",
    "web": "npx expo start --web",
    "android": "npx expo run:android",
    "android:dev": "npx expo start --clear && npx expo run:android",
    "ios": "npx expo run:ios",
    "lint": "eslint . --fix",
    "clean": "rm -rf node_modules && rm -rf .expo && rm -rf dist && npm install --legacy-peer-deps",
    "clean:windows": "rmdir /s /q node_modules && rmdir /s /q .expo && rmdir /s /q dist && npm install --legacy-peer-deps",
    "reset-metro": "npx react-native start --reset-cache"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@nozbe/watermelondb": "^0.28.0",
    "@nozbe/with-observables": "^1.6.0",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-picker/picker": "^2.11.0",
    "@react-navigation/drawer": "^7.3.9",
    "@supabase/supabase-js": "^2.46.1",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "ajv-keywords": "^5.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "expo": "~52.0.46",
    "expo-constants": "~17.0.8",
    "expo-font": "^13.0.4",
    "expo-image": "~2.0.7",
    "expo-linking": "~7.0.3",
    "expo-router": "~4.0.20",
    "expo-secure-store": "~14.0.0",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.9",
    "lucide-react-native": "^0.488.0",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.55.0",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "^15.8.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.13",
    "tailwind-merge": "^2.5.4",
    "tailwindcss": "^3.4.14",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.3.12",
